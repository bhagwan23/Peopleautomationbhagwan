<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite  name="Go.Deskera Web BVT Suite"
	parallel="none" preserve-order="true">

	<listeners>
		<listener
			class-name="deskera.web.automation.core.TestListenerOnFailure2">
		</listener>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="singUpUrl"
		value="https://auth-qa.deskera.xyz/auth/signup"></parameter>
	<parameter name="url"
		value="https://auth-qa.deskera.xyz/auth/signin?app=GO"></parameter>

	<!-- System config for framework -->
	<parameter name="envConf"
		value="src/test/resources/environment.config"></parameter>

	<!-- Test data US -->
	<parameter name="conf"
		value="src/test/resources/TestDataUS.config"></parameter>

	<!-- Run Test Cases -->

 	 <test name="C16960 ,C20565,C20566 Go.Deskera Sign Up test"
		parallel="false">
<!-- 		US specific test data
 -->		<classes>
			<class name="deskera.web.automation.tests.SignUpTest">
				<methods>
					<include name="testSignUpPage"></include>
					<include name="testSignUpForDeskeraBookkeeper"></include>
					<include name="testSignUpWithoutPhoneNumber"></include>
				</methods>
			</class>
		</classes>
	</test>
 
