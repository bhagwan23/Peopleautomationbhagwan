<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite thread-count="1" name="Go.Deskera Web BVT Suite"
	parallel="false">

	<listeners>
		<listener
			class-name="deskera.web.automation.core.TestListenerOnFailure">
		</listener>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url"
		value="https://auth-qa.deskera.xyz/auth/signin"></parameter>

	<!-- System config for framework -->
	<parameter name="envConf"
		value="src/test/resources/systemConfig/environment.config"></parameter>

	<parameter name="singUpUrl"
		value="https://auth-qa.deskera.xyz/auth/signup"></parameter>

	<!-- Run Test Cases -->
	<parameter name="conf"
		value="src/test/resources/goConfig/TestDataUS.config"></parameter>
	<test name="C16960 Go.Deskera Sign Up test" parallel="false">
		<classes>
			<class name="deskera.web.automation.go.tests.SignUpTest">
				<methods>
					<include name="testSignUpPage"></include>

				</methods>
			</class>
		</classes>
	</test>
	
	 <test name="C20565 Go.Deskera Sign Up as Bookkeeper test"
		parallel="false">
		<classes>
			<class name="deskera.web.automation.go.tests.SignUpTest">
				<methods>

					<include name="testSignUpForDeskeraBookkeeper"></include>

				</methods>
			</class>
		</classes>
	</test>


	<test name="C20566 Go.Deskera Sign Up without Phone Number test"
		parallel="false">
		<classes>
			<class name="deskera.web.automation.go.tests.SignUpTest">
				<methods>

					<include name="testSignUpWithoutPhoneNumber"></include>
				</methods>
			</class>
		</classes>
	</test> 


	<test name="C20564 Sign In" parallel="false">

		
		<classes>
			<class name="deskera.web.automation.go.tests.LoginTest">

			</class>
		</classes>
	</test>



</suite> 