	</test>
	
	<test name=" 4 249080 To verify user should be able to import Chart of Accounts" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.LoginTest">
				<methods>
					<include name="loginTest"></include>
				</methods>
			</class>
			<class
				name="deskera.web.automation.erp.imports.tests.Chartofaccountsimporttest">
				
