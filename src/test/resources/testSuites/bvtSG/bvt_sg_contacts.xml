						value="src/test/resources/bvtSGConfig/TestDataContactSG.config"></parameter>
					<include name="deleteContacts"></include>
				</methods>
			</class>
		</classes>
	</test>
    
<test name="5 C20261 Create Custom Number Format" parallel="false">
