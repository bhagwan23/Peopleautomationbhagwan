<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite thread-count="1" name="Go.Deskera Web BVT Suite" parallel="false">

	<listeners>
		<listener class-name="deskera.web.automation.core.TestListenerOnFailure">
		</listener>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url"
		value="https://auth-qa.deskera.xyz/auth/signin?redirectUrl=https:%2F%2Freality-qa.deskera.xyz"></parameter>

	<!-- System config for framework -->
	<parameter name="envConf"
		value="src/test/resources/systemConfig/environment.config"></parameter>

	<parameter name="singUpUrl" value="https://auth-qa.deskera.xyz/auth/signup"></parameter>

	<!-- Run Test Cases -->
	<parameter name="conf"
		value="src/test/resources/bvtSGConfig/TestDataSG.config"></parameter>


	<test name="C16960 Deskera Books Sign Up test" enabled="false"
		parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.SignUpTest">
				<methods>
					<include name="testSignUpPage"></include>

				</methods>
			</class>
		</classes>
	</test>

	<test name="C20565 Deskera books Sign Up as Bookkeeper test"
		enabled="false" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.SignUpTest">
				<methods>

					<include name="testSignUpForDeskeraBookkeeper"></include>

				</methods>
			</class>
		</classes>
	</test>


	<test name="C20566 Deskera books Sign Up without Phone Number test"
		enabled="false" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.SignUpTest">
				<methods>

					<include name="testSignUpWithoutPhoneNumber"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="C20564 Sign In" enabled="false" parallel="false">


		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.LoginTest">
				<methods>

					<include name="loginTest"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="C20261 Add Contacts" enabled="false" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.ContactsTest">
				<methods>
					<!--<include name="addContacts"></include> -->
					<include name="addContacts1"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="C20261 Edit Contacts" enabled="false" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.EditContactTest">
				<methods>
					<include name="editContacts"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="C20261 Copy Contacts"   parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.CopyContactTest">
				<methods>
					<include name="copyContacts"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="C20261 Delete Contacts" enabled="false" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.DeleteContactTest">
				<methods>
					<include name="deleteContacts"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="C20274 To verify that user is able to Create/Connect - Bank"
		enabled="false" parallel="false">
		<classes>
			<class name="deskera.web.automation.erp.bvtSG.tests.CreateBankTest">
				<methods>
					<include name="addBank"></include>
				</methods>
			</class>
		</classes>
	</test>

</suite> 